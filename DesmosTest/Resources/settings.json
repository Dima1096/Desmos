{
  "environment": "Stage",
  "browserName": "chrome",
  "isRemote": false,
  "remoteConnectionUrl": "",
  "isElementHighlightEnabled": true,


  "driverSettings": {
    "chrome": {
      "webDriverVersion": "MatchingBrowser",
      "capabilities": {
        "enableVNC": true,
        "unhandledPromptBehavior": "default"
      },
      "options": {
        "intl.accept_languages": "en",
        "safebrowsing.enabled": "true",
        "profile.default_content_settings.popups": "0",
        "disable-popup-blocking": "true",
        "download.prompt_for_download": "false",
        "download.default_directory": "./downloads"
      },
      "startArguments": [
        "window-size=1920,1080",
        "--no-sandbox",
        "--headless",
        "--lang=en"
      ]
    }
  },
  "timeouts": {
    "timeoutImplicit": 0,
    "timeoutCondition": 35,
    "timeoutScript": 15,
    "timeoutPageLoad": 20,
    "timeoutPollingInterval": 300,
    "timeoutCommand": 60,
    "timeoutElementAppear": 20
  },
  "retry": {
    "number": 2,
    "pollingInterval": 300
  },
  "logger": {
    "language": "en",
    "logPageSource": true
  },
  "elementCache": {
    "isEnabled": false
  },
  "visualization": {
    "defaultThreshold": 0.012,
    "comparisonWidth": 16,
    "comparisonHeight": 16,
    "pathToDumps": "Resources/VisualDumps/"
  }
}
